function InitializeGenericFAQsWidgetDropdown(){$("#genericFAQsWidgetDropdown .dropdown ").on("hide.bs.dropdown",function(){var n=$(this).parent().parent().find("select").val();let i=$(this).parent().parent().find("#txtGUID").val(),t=".faq-collapse.guid-"+i;$(t).css("display","none");n=="0"?$(t).css("display","block"):$("."+n).css("display","block")})}$(document).ready(function(){$("#claimsFaqsTopicDropdwon .dropdown ").on("hide.bs.dropdown",function(){var n=$(this).parent().parent().find("select").val();$(".faq-collapse").css("display","none");n=="0"?$(".faq-collapse").css("display","block"):$("."+n).css("display","block")})});$(document).ready(function(){$("#btnClaimsProcess").on("click",function(){$(".claimsReasonDocumentForm").css("display","none");let t,u=$("#IsMultiSelectClaimsReason").val();t=u?$("#claimsReasonCheckInputForms").find('input[type="checkbox"]:checked'):$("#claimsReasonCheckInputForms").find('input[type="radio"]:checked');t.length>0&&$.each(t,function(){let n=$(this).val(),t=$("#claimsSubmissionForm").find('input[type="hidden"]'),i=$("#claimsThingsToEnsureForm").find('input[type="hidden"]');$.each(i,function(){if($(this).attr("data-claimreason")==n){let n=$(this).val();$("#"+n).css("display","block")}});$.each(t,function(){if($(this).attr("data-claimreason")==n){let n=$(this).val();$("#"+n).css("display","block")}})});let i=document.querySelectorAll(".num"),n=1,r=!1;$(".claimsReasonDocumentForm").each(function(){$(this).attr("style").toString().indexOf("none")===-1&&$(this).find(".rte-link").length&&(r=!0)});r?(document.querySelector("#claimsThingsToEnsureForm").classList.remove("d-none"),n+=1,i[1].innerHTML=n):document.querySelector("#claimsThingsToEnsureForm").classList.add("d-none");let f=document.querySelector('.claimsReasonDocumentForm[style="display: block;"]');f===null?document.querySelector("#claimsSubmissionForm").classList.add("d-none"):(document.querySelector("#claimsSubmissionForm").classList.remove("d-none"),n+=1,i[2].innerHTML=n);i[3].innerHTML=n+1});$("#ddlClaimsTypeItems .dropdown ").on("hidden.bs.dropdown",function(){var n=$(this).parent().parent().find("select").val();n!="#"&&window.open(n,"_self");$(this).parent().parent().find("select").val("#")})});$(document).ready(function(){$("#faqContactusTopicddl .dropdown ").on("hide.bs.dropdown",function(){var n=$(this).parent().parent().find("select").val(),t=$(this).parent().parent().find("select option:selected").attr("data-groupid");$("."+t).css("display","none");$("."+n).css("display","block")})});$(document).ready(function(){$("#FaqsTopicDropdwon .dropdown ").on("hide.bs.dropdown",function(){var n=$(this),t=n.parent().parent().find("select"),i=t.val(),r=t.find(":selected"),u=r.text(),f=n.find(".dropdown-toggle span");f.text(u);$(".faq-collapse").css("display","none");i==="0"?$(".faq-collapse").css("display","block"):$("."+i).css("display","block")})});$(document).ready(function(){InitializeGenericFAQsWidgetDropdown()});$(document).ready(function(){$("#dropComp .dropdown ").on("hide.bs.dropdown",function(){var n=document.querySelector("#dropComp select").value,t,i;n&&($(".catname").css("display","none"),$(".catname span").hasClass(n)&&(t=" ."+n,$(".catname span").filter(t).parent().css("display","block"),$("div .highlight").css("display","none"),i="div ."+n,$(i).css("display","block")));setTitleHeight(".compSameHeight")});$("#ddlfirstTimeLoad .dropdown ").on("hide.bs.dropdown",function(){var n=document.querySelector("#ddlfirstTimeLoad select").value,r=$("#ddlfirstTimeLoad select option:selected").text(),t,i;$("#dropComp .dropdown-toggle > span").text(r);$("#dropComp select").val(n);$("#_card2").hasClass("d-none")&&($("#_card2").removeClass("d-none"),$("#_card1").addClass("d-none"));n&&($(".catname").css("display","none"),$(".catname span").hasClass(n)&&(t=" ."+n,$(".catname span").filter(t).parent().css("display","block"),$("div .highlight").css("display","none"),i="div ."+n,$(i).css("display","block")));setTitleHeight(".compSameHeight")})})